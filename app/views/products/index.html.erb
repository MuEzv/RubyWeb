

<p style="color: green"><%= notice %></p>

<% if session[:user_id] %>
  <div>
    <%= link_to "New product", new_product_path %>
  </div>
<% end%>
<h1>Products</h1>

<div id="products">
  <% @products.each do |product| %>
    <%= render product %>
    <%#
    <p><%= link_to "Show this product", product %><%#
    </p>
    %>
  <% end %>
</div>


